{
  "name": "dig-its",
  "description": "A game with digits",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "github:dig-its/dig-its",
  "scripts": {
    "version": "echo \"export default '`git rev-parse HEAD`';\" > version.js",
    "dev": "yarn run version && next",
    "build": "yarn run version && next build",
    "export": "yarn run build && next export",
    "start": "next start",
    "test": "jest",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "serve": "yarn run export && npx http-server out/"
  },
  "dependencies": {
    "next": "^11.1.2",
    "object-path": "0.11.8",
    "prop-types": "^15.7.2",
    "raven-js": "^3.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fontawesome": "^1.7.1",
    "react-ga": "^3.3.0",
    "reactstrap": "^8.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "enzyme": "^3.11.0",
    "jest": "^27.2.1",
    "jest-localstorage-mock": "^2.4.17",
    "react-test-renderer": "^17.0.2"
  }
}
