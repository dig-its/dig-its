{
  "name": "dig-its",
  "description": "A game with digits",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "github:dig-its/dig-its",
  "scripts": {
    "version": "echo \"export default '`git rev-parse HEAD`';\" > version.js",
    "dev": "yarn run version && next",
    "build": "yarn run version && next build",
    "export": "yarn run build && next export",
    "start": "next start",
    "test": "jest",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "serve": "yarn run export && npx http-server out/"
  },
  "dependencies": {
    "next": "^9.5.5",
    "prop-types": "^15.7.2",
    "raven-js": "^3.27.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-fontawesome": "^1.7.1",
    "react-ga": "^2.7.0",
    "reactstrap": "^8.4.1"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^25.4.0",
    "jest-localstorage-mock": "^2.4.0",
    "react-test-renderer": "^16.13.1"
  },
  "resolutions": {
    "jsdom": "^15.2.1",
    "object-path": "^0.11"
  }
}
